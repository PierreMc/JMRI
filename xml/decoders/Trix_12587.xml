<decoder-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" showEmptyPanes="no" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">
  <version version="1" author="PB, pierre.bln@me.com" lastUpdated="20130807"/>
  <!-- 1 2013/07/24 Creation 
		This is the first decoder template for a series of Minitrix integrated
		decoders. Those decoders are manufactured by Doehler and Haass (in most
		cases). However, CVs and functionalities are often different from the 
		original Doehler and Haass decoders. Therefore, these decoder definitions
		are not linked to the Doehler and Haass definitions.
		THIS FILE CONTAINS:
		Minitrix 12587 (Lint Diesel Motorized Unit).
		Based on: http://www.maile.es/PDF/DHS240-MTX_LINT_Datenblatt.pdf
  -->
  <decoder>
    <family name="Minitrix on-board decoders" mfg="Trix Modelleisenbahn">
      <model model="12587 (Lint with sound)" productID="12587" comment="Decoder base Doehler Haass DHS240">
		<protocols>
            <protocol>dcc</protocol>
            <protocol>selectrix</protocol>
        </protocols>
      </model>
    </family>
    <programming direct="yes" paged="no" register="no" ops="yes"/>
    <variables>
	<!-- Calls to std NMRA templates -->
		<xi:include href="http://jmri.org/xml/decoders/nmra/shortAndLongAddress.xml"/>
		<xi:include href="http://jmri.org/xml/decoders/nmra/cv29direction.xml"/>
		<xi:include href="http://jmri.org/xml/decoders/nmra/cv29analog.xml"/>
		<xi:include href="http://jmri.org/xml/decoders/nmra/mfgVersionId.xml"/>
		<xi:include href="http://jmri.org/xml/decoders/nmra/accelDecel.xml"/>
		<xi:include href="http://jmri.org/xml/decoders/nmra/cv29speedSteps.xml"/>
	  	<!-- Standard motor options -->
		<variable label="Vhigh" item="Vhigh" CV="5" tooltip="CV5 (1-7)" default="7">
			<decVal min="1" max="7"/>
			<label>Vhigh</label>
			<label xml:lang="it">Volt Massimo</label>
			<label xml:lang="fr">Vmax</label>
			<label xml:lang="de">Höchstgeschwindigkeit (Vmax)</label>
		</variable>
	  <!-- EMF options -->
	  	<variable label="Regulation variant (do not modify!)" CV="50" default="2" item="Load Control" tooltip="0=very hard, 1=hard, 2=soft, 3=very soft">
			<label xml:lang="fr">Variante de réglage (ne pas changer!)</label>
			<label xml:lang="de">Regelvariante (nicht verändern!)</label>
			<enumVal>
				<enumChoice choice="Very Hard" value="0" >
					<choice>Very Hard</choice>
					<choice xml:lang="fr">Très dur</choice>
					<choice xml:lang="de">Sehr Hart</choice>
				</enumChoice>
				<enumChoice choice="Hard" value="1">
					<choice>Hard</choice>
					<choice xml:lang="fr">Dur</choice>
					<choice xml:lang="de">Hart</choice>
				</enumChoice>
				<enumChoice choice="Soft" value="2">
					<choice>Soft</choice>
					<choice xml:lang="fr">Souple</choice>
					<choice xml:lang="de">Weich</choice>
				</enumChoice>
				<enumChoice choice="Very soft" value="3">
					<choice>Very soft</choice>
					<choice xml:lang="fr">Très souple</choice>
					<choice xml:lang="de">Sehr weich</choice>
				</enumChoice>
			</enumVal>
		  </variable>
		  <variable label="Impulse width (do not modify!)" CV="49" default="2" item="EMF Option 1" tooltip="0=small, 1=normal, 2=wide, 3=very wide">
			<decVal  min="0" max="3"/>
			<label xml:lang="fr">Largeur d'impulsion (ne pas changer!)</label>
			<label xml:lang="de">Impulsbreite (nicht verändern!)</label>
		  </variable>
	<!-- Advanced panel -->
		<variable label="Invert motor pins" CV="51" mask="XXXXXXXV" readOnly="yes" item="Advanced Group 1 Option 1">
			<decVal/>
			<label xml:lang="fr">Permuter connexion moteur</label>
			<label xml:lang="de">Motoranschlüsse vertauschen</label>
	  	</variable>
	  	<variable label="Invert light pins" CV="51" mask="XXXXXXVX" readOnly="yes" item="Advanced Group 1 Option 2">
			<decVal/>
			<label xml:lang="fr">Permuter connexion feux</label>
			<label xml:lang="de">Lichtanschlüsse vertauschen</label>
	  	</variable>
